                    </div>

                    <div class="md-form mt-3">
                        <textarea type="text" id="userText" class="md-textarea form-control" rows="5"></textarea>
                        <label for="userText">Data</label>
                    </div>
                    <input type="hidden" id="hdnAction" value="compare">
                    <!-- Submit button -->
